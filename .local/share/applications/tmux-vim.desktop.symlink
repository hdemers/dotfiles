# vim: set ft=desktop:
[Desktop Entry]
Type=Application
Version=1.0
Name=tmux-vim
GenericName=Vim in tmux
Comment=Vim running in tmux running in a terminal
Icon=/var/home/hdemers/.local/share/icons/neovim.svg
Exec=/home/linuxbrew/.linuxbrew/bin/wezterm start tmux new-session -AD -t tmux-vim -s tmux-vim

StartupNotify=true
Terminal=false
Categories=Utility;ConsoleOnly;
Keywords=editor;vim;terminal;tmux
StartupNotify=true
