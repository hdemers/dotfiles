# vim: set ft=toml:

[aws]
disabled = false
style = 'yellow'
symbol = 'aws:'

[python]
symbol = 'py '
style = 'yellow'

[directory]
style = 'cyan'

[git_branch]
style = 'purple'

[git_commit]
style = 'green'

[git_state]
style = 'yellow'

[git_status]
style = 'red'

[username]
style_root = 'red'
style_user = 'yellow'

[rlang]
detect_files = []
symbol = 'R '

[shell]
bash_indicator = 'Â·'
zsh_indicator = ''
format = '[$indicator]($style)'
style = 'green'
disabled = false

